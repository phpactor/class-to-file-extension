{
    "name": "phpactor/class-to-file-extension",
    "type": "phpactor-extension",
    "description": "Converts classes to files and vice-versa",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "phpactor/class-to-file": "^0.4.0",
        "phpactor/composer-autoloader-extension": "^0.2.3",
        "phpactor/container": "^2.0.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "^9.0",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        },
        "phpactor.extension_class": "Phpactor\\Extension\\ClassToFile\\ClassToFileExtension"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\ClassToFile\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\ClassToFile\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
